<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mrizkifadil26.bioapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_biologo_480"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_biologo_480"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.PanganDetailActivity"
            android:label="@string/title_activity_detail"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".activities.PanganList"></activity>
    </application>

</manifest>